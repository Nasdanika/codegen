  <eClassifiers xsi:type="ecore:EClass" name="Generator" abstract="true" eSuperTypes="../../org.nasdanika.ncore/model/ncore.ecore#//Entity ../../org.nasdanika.ncore/model/ncore.ecore#//Configurable">
